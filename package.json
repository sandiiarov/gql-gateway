{
  "name": "gql-gateway",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "start": "tsc && node dist/src/index.js",
    "dev": "tsnd src/index.ts",
    "generate:schema": "get-graphql-schema http://localhost:4000/graphql > src/schema/schema.graphql",
    "generate:types": "gql-gen"
  },
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "head/master",
      "path": "src/schema/schema.graphql"
    }
  },
  "dependencies": {
    "apollo-engine": "1.1.2",
    "apollo-link-http": "1.5.11",
    "apollo-server-express": "2.3.3",
    "express": "4.16.4",
    "graphql": "14.1.1",
    "graphql-tools": "4.0.4",
    "isomorphic-fetch": "2.2.1"
  },
  "devDependencies": {
    "@types/express": "4.16.1",
    "@types/graphql": "14.0.5",
    "@types/isomorphic-fetch": "0.0.34",
    "@typescript-eslint/eslint-plugin": "1.2.0",
    "eslint": "5.13.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "get-graphql-schema": "2.1.2",
    "graphql-code-generator": "0.16.1",
    "graphql-codegen-typescript-common": "0.16.1",
    "graphql-codegen-typescript-resolvers": "0.16.1",
    "prettier": "1.16.4",
    "ts-node-dev": "1.0.0-pre.32",
    "typescript": "3.2.1"
  }
}
